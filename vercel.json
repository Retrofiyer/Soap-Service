{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "db.json",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/saveUser",
            "dest": "/index.js"
        },
        {
            "src": "/getUsers",
            "dest": "/index.js"
        },
        {
            "src": "/wsdl",
            "dest": "/index.js"
        },
        {
            "src": "/wsdl(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}