{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "db.json",
        "use": "@vercel/static"
      },
      {
        "src": "service.wsdl",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/saveUser",
        "methods": ["POST"],
        "dest": "/index.js"
      },
      {
        "src": "/getUsers",
        "methods": ["GET"],
        "dest": "/index.js"
      },
      {
        "src": "/wsdl",
        "methods": ["GET", "POST"],
        "dest": "/index.js"
      }
    ]
  }  