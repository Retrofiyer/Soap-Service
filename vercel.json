{
    "version": 2,
    "builds": [
        {
          "src":"./server.js",
          "use":"@vercel/node"
        },
        {
          "src":"./index.html",
          "use":"@vercel/static"
        }
    ],
    "routes": [
        {
            "src":"/saveUser",
            "dest":"server.js"
        },
        {
            "src":"/getUsers",
            "dest":"server.js"
        },
        {
            "src":"/wsdl(.*)",
            "dest":"server.js"
        },
        {
            "src":"/(.*)",
            "dest":"/index.html"
        }
    ]
}